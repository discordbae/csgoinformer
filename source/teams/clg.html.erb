            <th>Steam ID</th>
            <th>Name</th>
            <th>Roles</th>
            <th>Country</th>
            <th>Join Date</th>
          </tr>
        </thead>
        <tbody>
          <% content_tag :tr, class: "team-list-player",
            data: { player_id: "76561197960784488", player_name: "reltuC" } do %>
            <td>reltuC</td>
