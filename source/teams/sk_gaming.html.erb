            <th>Name</th>
            <th>Roles</th>
            <th>Country</th>
            <th>Join Date</th>
          </tr>
        </thead>
        <tbody>
          <% content_tag :tr, class: "players-list-player",
            data: { player_id: "76561197960690195", player_name: "FalleN" } do %>
            <td>FalleN</td>
            <td>Gabriel Toledo</td>
            <td>IGL / AWPer</td>
            <td>Brazil</td>
            <td>2016-07-01</td>
          <% end %>
          <% content_tag :tr, class: "players-list-player",
